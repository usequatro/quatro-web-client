# .env.development is used during development
# .env is used when deploying to production

# For Google Analytics.
REACT_APP_GA_MEASUREMENT_ID=UA-145783714-2

# Flag to indicate that it's local development.
REACT_APP_DEVELOPMENT=1

# Allows using src/ as an absolute import path for components
# so we don't need to do ../../ while importing.
NODE_PATH=src/